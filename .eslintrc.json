{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard",
        "prettier",
        "plugin:prettier/recommended",
        "prettier/react"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "settings": {
      "react": {
        "version": "latest"
      }
    },
    "ignorePatterns": ["package.json", "**/dist/*.js"],
    "rules": {
    }
}



